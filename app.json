{
  "expo": {
    "name": "thryvyng-mobile",
    "slug": "thryvyng-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "thryvyng",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.thryvyng.app",
      "associatedDomains": [
        "applinks:thryvyng.com",
        "applinks:www.thryvyng.com"
      ]
    },
    "android": {
      "package": "com.thryvyng.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "useNextNotificationsApi": true,
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "thryvyng.com",
              "pathPrefix": "/join-team"
            },
            {
              "scheme": "https",
              "host": "thryvyng.com",
              "pathPrefix": "/join-staff"
            },
            {
              "scheme": "https",
              "host": "thryvyng.com",
              "pathPrefix": "/register"
            },
            {
              "scheme": "https",
              "host": "thryvyng.com",
              "pathPrefix": "/accept-coparent"
            },
            {
              "scheme": "https",
              "host": "thryvyng.com",
              "pathPrefix": "/claim-player"
            },
            {
              "scheme": "https",
              "host": "thryvyng.com",
              "pathPrefix": "/invitation"
            },
            {
              "scheme": "https",
              "host": "thryvyng.com",
              "pathPrefix": "/invitations"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "color": "#8b5cf6",
          "sounds": []
        }
      ]
    ]
  }
}
